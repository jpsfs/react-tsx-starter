{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "./www/server.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": ".",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": "./www"
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858
		},
        {
            "name": "Launch TypeScript",
            "type": "node",
            "request": "launch",
            "program": "src/server.tsx",
            "sourceMaps": true,
            "outDir": "www"
        }
	]
}